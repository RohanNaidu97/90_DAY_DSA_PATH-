
########################################################################## MY SOLUTION ##############################################################

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int test, ele, candy, happy;
	cin>>test;
	for(int i = 0; i < test; i++)
	{
	    cin>>ele>>candy;
	    
	    while(ele)
	    {
	        cin>>happy;
	        candy -= happy;
	        ele--;
	    }
	    
	    if(candy >= 0)
	        cout<<"Yes"<<endl;
	  
	    else
	        cout<<"No"<<endl;
	}
	return 0;
}
